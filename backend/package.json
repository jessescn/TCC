{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_PATH=src NODE_ENV=dev nodemon -r dotenv/config --exec ts-node src/index.ts --watch src",
    "start": "yarn build && cd dist/ && NODE_PATH=dist/src NODE_ENV=prod node index.js",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "NODE_PATH=src jest",
    "tw": "NODE_PATH=src jest --watch --coverage",
    "lint": "eslint --ext .js,.ts .",
    "coverage": "NODE_PATH=src jest --coverage",
    "clear": "rm -rf ./db-data",
    "deploy": "gcloud run deploy --env-vars-file=.env.service.yaml",
    "local-deploy": "gcloud beta code dev --dockerfile=Dockerfile --local-port=8080"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/bcrypt": "5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.27",
    "@types/nodemailer": "^6.4.4",
    "@types/pdfkit": "^0.12.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.0",
    "jest-ts-auto-mock": "^2.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "tscpaths": "^0.0.9",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "bcrypt": "5.0.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "fast-csv": "^4.3.6",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "pdfkit": "^0.13.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.0"
  }
}
